{
  "name": "react-fancy-modal",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/react-fancy-modal.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "sass": "node-sass scss/main.scss dist/ReactFancyModal.css",
    "postsass": "postcss dist/ReactFancyModal.css --use autoprefixer -m -o dist/ReactFancyModal.css",
    "style": "npm run sass"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "autoprefixer": "^9.6.0",
    "husky": "^2.7.0",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0",
    "typescript": "^3.5.2"
  }
}
